import java.util.ArrayList;
import java.util.List;

public class model {
    private List<Post> Posts;

    public AppModel() {
        this.posts = new ArrayList<>();
    }

    public void addPost(Post post) {
        posts.add(post);
    }

    public List<Post> getPosts() {
        return posts;
    }
}

